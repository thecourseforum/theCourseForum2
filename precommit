# Auto format.
docker exec tcf_django autopep8 --in-place --aggressive --aggressive -r tcf_website tcf_core
# Lint.
docker exec tcf_django pylint --load-plugins pylint_django --django-settings-module=tcf_core.settings tcf_website tcf_core
docker exec tcf_django node_modules/.bin/eslint --fix -c .config/.eslintrc.yml 'tcf_website/static/' 
# Test.
docker exec tcf_django coverage run manage.py test
# Get code coverage.
docker exec tcf_django coverage report -m
